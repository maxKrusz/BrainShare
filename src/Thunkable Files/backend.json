{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "users": {
      "type": "object",
      "patternProperties": {
        "^[0-9a-f]{30}$": {
          "type": "object",
          "properties": {
            "firstName": {
              "type": "string"
            },
            "lastName": {
              "type": "string"
            }
          },
          "required": ["firstName", "lastName"]
        }
      }
    }
  },
  "required": ["users"]
}
